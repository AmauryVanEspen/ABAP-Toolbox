  <method CLSNAME="ZCL_BDC" CMPNAME="ADD_MAIN_SCREEN" VERSION="1" LANGU="P" DESCRIPT="Add transaction main screen to BDC Table" EXPOSURE="2" STATE="1" EDITORDER="14 " DISPID="0 " AUTHOR="NKIMOTO" CREATEDON="20120322" CHANGEDBY="NKIMOTO" CHANGEDON="20120327" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZCL_BDC" CMPNAME="ADD_MAIN_SCREEN" SCONAME="PROGRAM" VERSION="1" LANGU="P" DESCRIPT="Pool de módulos BDC" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="NKIMOTO" CREATEDON="20120322" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BDC_PROG" PAROPTIONL="X"/>
   <parameter CLSNAME="ZCL_BDC" CMPNAME="ADD_MAIN_SCREEN" SCONAME="DYNPRO" VERSION="1" LANGU="P" DESCRIPT="NÚmero de tela BDC" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="NKIMOTO" CREATEDON="20120322" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BDC_DYNR" PAROPTIONL="X"/>
   <parameter CLSNAME="ZCL_BDC" CMPNAME="ADD_MAIN_SCREEN" SCONAME="DYNBEGIN" VERSION="1" LANGU="P" DESCRIPT="Início BDC de uma tela" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="NKIMOTO" CREATEDON="20120322" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BDC_START" PARVALUE="&apos;X&apos;"/>
   <source>METHOD add_main_screen.

  IF program IS INITIAL.

    APPEND me-&gt;wa_main_screen TO me-&gt;t_bdcdata.

  ELSE.

    me-&gt;wa_bdc-program  = program.
    me-&gt;wa_bdc-dynpro   = dynpro.
    me-&gt;wa_bdc-dynbegin = dynbegin.
    me-&gt;wa_main_screen  = me-&gt;wa_bdc.
    me-&gt;wa_last_screen  = me-&gt;wa_bdc.
    APPEND me-&gt;wa_bdc TO me-&gt;t_bdcdata.
    CLEAR  me-&gt;wa_bdc.

  ENDIF.

ENDMETHOD.</source>
  </method>
